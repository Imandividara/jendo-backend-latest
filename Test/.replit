modules = ["nodejs-20", "java-graalvm22.3", "nix", "postgresql-16"]
[agent]
expertMode = true

[nix]
channel = "stable-25_05"

[workflows]
runButton = "Project"

[[workflows.workflow]]
name = "Project"
mode = "parallel"
author = "agent"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "Jendo Health App"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "Backend Server"

[[workflows.workflow]]
name = "Jendo Health App"
author = "agent"

[workflows.workflow.metadata]
outputType = "webview"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "cd \"Jendo-dev/Expo (3)/Expo\" && node proxy-server.js"
waitForPort = 5000

[[workflows.workflow]]
name = "Backend Server"
author = "agent"

[workflows.workflow.metadata]
outputType = "console"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "cd Jendo-dev/Jendo-Backend/Jendo-Backend && mvn spring-boot:run -Dspring-boot.run.jvmArguments=\"-Djava.awt.headless=true\""
waitForPort = 8080

[[ports]]
localPort = 5000
externalPort = 80

[[ports]]
localPort = 8080
externalPort = 8080

[[ports]]
localPort = 8081
externalPort = 8081

[[ports]]
localPort = 8082
externalPort = 3001

[[ports]]
localPort = 19006
externalPort = 3000

[deployment]
deploymentTarget = "autoscale"
run = ["bash", "-c", "cd 'Jendo-dev/Expo (3)/Expo' && npx serve dist -l 5000"]
build = ["bash", "-c", "cd 'Jendo-dev/Expo (3)/Expo' && npm install && npx expo export --platform web"]

